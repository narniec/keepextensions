{
	"manifest_version": 2,
	"name": "KEEP Bookmarks",
	"description": "KEEP Bookmarks",
	"version": "1.0.0",
	"icons": {"128": "icon_128.png"},
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"permissions": ["activeTab"],
	"content_security_policy": "script-src 'self' https://widgets.coingecko.com; object-src 'self'"
}
